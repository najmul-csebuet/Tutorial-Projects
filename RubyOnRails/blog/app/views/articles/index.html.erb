<h1>Articles from database</h1>

<ul>
    <% @articles.each do |article| %>
        <li>
            <a href = "/articles/<%= article.id %>">
                <%= article.title %>
            </a>
            <br/>
            <a href = "<%= article_path(article) %>">
                <%= article.title %>
            </a>
            <br/>
            <a href = "<%= article_url(article) %>">
                <%= article.title %>
            </a>
            <br/>
            <%= link_to article.title, article %>
        </li>
    <% end %>
</ul>

<%= link_to "New Article", new_article_path %>